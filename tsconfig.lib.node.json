{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES2019",
        "moduleResolution": "Node",
        "strict": true,
        "sourceMap": true,
        "skipLibCheck": true,
        "strictNullChecks": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strictPropertyInitialization": true,
        "strictFunctionTypes": true,
        "experimentalDecorators": true,
        "incremental": true,
    }
}
